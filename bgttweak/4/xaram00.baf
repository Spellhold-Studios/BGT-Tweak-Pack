IF
  PartyHasItem("CLCK33")  // Ring of Free Action
  Global("IHaveSemaj","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobal("IHaveSemaj","GLOBAL",1)
    Continue()
END

